<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D5E8EC">

    <TextView
        android:id="@+id/textViewD1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="12dp"
        android:text="Contact Num = "
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#050505"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewDID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="88dp"
        android:text="ContactNumber"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#050505"
        app:layout_constraintBaseline_toBaselineOf="@+id/textViewD1"
        app:layout_constraintBottom_toBottomOf="@+id/textViewD1"
        app:layout_constraintLeft_toRightOf="@+id/textViewD1" />

    <TextView
        android:id="@+id/textViewD2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="12dp"
        android:text="Name = "
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#050505"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewD1" />

    <TextView
        android:id="@+id/textViewDNAME"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="152dp"
        android:text="Name"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="#050505"
        app:layout_constraintBottom_toBottomOf="@+id/textViewD2"
        app:layout_constraintLeft_toRightOf="@+id/textViewD2" />


</androidx.constraintlayout.widget.ConstraintLayout>